<div class="row">

    <!-- main content-->
    <div class="col-md-12">
            <% if @cblogs.exists? %>
               <% @cblogs.each do |blog| %>
                 <div class="well">
                    <h3 class="page-header"><%= raw blog.title%></h3> 
                    <%= raw blog.body%> 
                 </div>
               <% end %>
            <% else %>
              <p>This part is under construction, it will be available as soon as possible</p>
            <% end %>
        
    </div><!-- End of main content -->
